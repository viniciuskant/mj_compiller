// Fibonacci
class Program {
    public static void main(String[] args) {
        int i, n, t1 = 0, t2 = 1, nextTerm;
        print("Enter the number of terms: ");
        n = 10;
        print("Fibonacci Series: ");

        for (i = 1; i <= n; i=i+1) {
            print(t1, " ");
            nextTerm = t1 + t2;
            t1 = t2;
            t2 = nextTerm;
        }
    }
}
