// print function
class Program {
    public int f(int m, int n) {
        if (! (n >= 2)) {
            n = n - 2;
            return n;
        }
        else {
            m = m -1;
            return m;
        }
    }

    public static void main(String[] args) {
        int m = -9;
        int n = 4;
        print(this.f(m, n));
    }
}
